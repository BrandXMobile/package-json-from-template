{
  "name": "package-json-from-template",
  "version": "0.0.3",
  "description": "Generate a package.json file from a template",
  "scripts": {
    "ava": "ava",
    "lint": "xo",
    "test": "xo & ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreasonny83/package-json-from-template.git"
  },
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "pack-gen": "./bin/cli.js"
  },
  "keywords": [
    "package",
    "package-json",
    "generator",
    "package-json-generator"
  ],
  "author": {
    "name": "Andrea SonnY",
    "email": "andreasonny83@gmail.com",
    "url": "https://andreasonny83.github.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreasonny83/package-json-from-template/issues"
  },
  "homepage": "https://github.com/andreasonny83/package-json-from-template#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "meow": "^4.0.0",
    "read-pkg": "^3.0.0",
    "replace-string": "^1.1.0",
    "write-pkg": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "del": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-config-xo": "^0.19.0",
    "execa": "^0.8.0",
    "load-json-file": "^4.0.0",
    "tempfile": "^2.0.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "only-multiline"
      ]
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-xo"
  }
}
